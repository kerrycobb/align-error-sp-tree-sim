@article{lambertInferringIntrogressionUsing2019,
	title = {Inferring introgression using {RADseq} and \textit{{D}} $_{\textrm{{FOIL}}}$ : {Power} and pitfalls revealed in a case study of spiny lizards ( \textit{{Sceloporus}} )},
